#include <iostream>

using namespace std;

int main()
{
    int i, N, total, angka;

    cout << "Jumlah angka   = ";
    cin >> N;
    cout << endl;

    total = 0;

    for (i=1; i<=N; i++)

        {
        cout << "Input angka   = ";
        cin >> angka;
        cout << endl;

        total = total + angka;
    }
        cout << "Total  = " << total << endl;

    return 0;
}
